{
  "name": "kynx/guzzle-expressive-cookiejar",
  "description": "Guzzle cookiejar with zend-expressive-session persistence",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Matt Kynaston",
      "email": "matt@kynx.org"
    }
  ],
  "keywords": [
    "cookiejar",
    "guzzle",
    "zend",
    "expressive"
  ],
  "autoload": {
    "psr-4": {
      "Kynx\\Guzzle\\Expressive\\":"src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KynxTest\\Guzzle\\Expressive\\":"test/"
    }
  },
  "require": {
    "php":">=7.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3",
    "zendframework/zend-expressive-session": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "zendframework/zend-coding-standard": "^1.0"
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "check": [
      "@cs-check",
      "@test"
    ],
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "upload-coverage": "coveralls -v"
  }
}